<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zpop.web.dao.ProfileFileDao">
    <insert id="insert" parameterType="ProfileFile"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO profileFile(name)
        VALUES (#{name})
    </insert>
</mapper>